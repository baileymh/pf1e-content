{"_id":"B5iKKSAoHAbpvZqO","name":"Bent Body (Tiefling; Beastbrood)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>While nothing is actually reversed, your bones and limbs are slightly out of alignment.</p></em>\n<p>&nbsp;</p>\n<p><strong>Requirements</strong></p>\n<p>Beastbrood Tielfling Heritage</p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p>You gain a +2 trait bonus to CMD when resisting grapple attempts.</p>","chat":"","unidentified":""},"tags":[["Race"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"bentBodyTieflingBeastbrood","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Bent Body (Tiefling; Beastbrood)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"systems/pf1/icons/skills/blue_35.jpg","effects":[]}
{"_id":"B9EhwYGcPP8d2e3p","name":"Jungle Fighter (Sargavan)","permission":{"default":0,"zVsAG968dy8xHxpn":3},"type":"feat","data":{"description":{"value":"<p><em>You are sure-footed in jungle environments.</p></em>\n<p>&nbsp;</p>\n<p><span id=\"ctl00_MainContent_DataListTraits_ctl00_LabelName\"><strong>Requirements</strong></span></p>\n<p><span id=\"ctl00_MainContent_DataListTraits_ctl00_LabelName\">Sargavan</span></p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTraits_ctl00_LabelName\">Your speed may not be reduced to less than 10 feet by difficult terrain in jungles (meaning you may take a 5-foot step in difficult terrain and not provoke an attack of opportunity).</span></p>","chat":"","unidentified":""},"tags":[["Combat"]],"activation":{"cost":1,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"jungleFighterSargavan","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[