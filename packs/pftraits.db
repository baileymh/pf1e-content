{"_id":"4lOnhM5NsYmzQtYo","name":"Pragmatic Activator","permission":{"default":0,"zVsAG968dy8xHxpn":3},"type":"feat","data":{"description":{"value":"<p><em><span id=\"ctl00_MainContent_DataListTypes_ctl87_LabelType\">While some figure out how to use magical devices with stubborn resolve, your approach is more pragmatic.</span></em></p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl87_LabelType\">You may use your Intelligence modifier when making Use Magic Device checks instead of your Charisma modifier.</span></p>","chat":"","unidentified":""},"tags":[["Magic"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"1FamilyTemplate","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Pragmatic Activator"},"flags":{},"img":"icons/sundries/bo