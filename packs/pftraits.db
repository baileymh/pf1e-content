{"_id":"9hRvqHG89OmRO9Tv","name":"Lover of the Law (Abadar)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>You have a strong belief in the righteousness of law and justice.\n\t</p></i><b>Benefit</b><p>You receive a +1 trait bonus on saves vs. charm and compulsion spells, and may make a new save with an additional +1 bonus (for a total of +2) if directed to act against the law.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Religion"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"loverOfTheLawAbadar","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Lover of the Law (Abadar)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"icons/environment/settlement/temple-night.webp","eff