{"_id":"xSXOS6Jp1AaI6Nye","name":"Aspiring Bard (Human)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>You spent countless hours of your youth wandering the open campuses of the Music Academy and the Choir College, listening to wonderful musicians and singers and daydreaming of your life as a bard.</em></p>\n<p><strong>Benefit</strong></p>\n<p>You gain a +1 trait bonus to one category of Perform checks and a +2 trait bonus to any Knowledge (local) checks that deal with the local music scene.</p>","chat":"","unidentified":""},"featType":"trait","tags":[["Race"]],"tag":"aspiringBardHuman","useCustomTag":false,"associations":{"classes":[]},"crOffset":"","conditionals":[],"identifiedName":"Aspiring Bard (Human)","abilityType":"none","showInQuickbar":false,"nonlethal":false,"activation":{"cost":null,"type":""},"actionType":"","soundEffect":"","measureTemplate":{"type":""},"changeFlags":{"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false,"loseDexToAC":false,"noDex":false,"noStr":false,"oneInt":false,"oneWis":false,"oneCha":false},"damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"contextNotes":[],"uses":{"value":0,"max":0}},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"systems/pf1/icons/skills/blue_35.jpg","effects":[]}
