{"_id":"axLUBMzSveZq461Z","name":"Mother's Teeth (Lamashtu)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>Your teeth are more jagged and pointed than normal.\n\t</p></i><b>Benefit</b><p>You can make a bite attack for 1d2 points of damage as a secondary attack.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Religion"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"mothersTeethLamashtu","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Mother's Teeth (Lamashtu)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"icons/environment/settlement/temple-night.webp","effects":[]}
{"_id":"az0Qb7bCgjuIojI1","name":"Calistrian Prostitute (Calistria)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>You worked in one of Calistria's temples as a sacred prostitute, and you know how to flatter, please, and (most of all) listen. </p></i><b>Benefit</b><p>You gain a +1 trait bonus on Sense Motive checks and Diplomacy checks to gather information, and one of these skills (your choice) is always a class skill for you. .</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Religion"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"calistrianProstituteCalistria","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Calistrian Prostitute (Calistria)"},"folder":"ex0HRF3LEDFUoJVd