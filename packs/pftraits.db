{"_id":"IJHgaOhnnemCFhXe","name":"Wiscrani Ear","permission":{"default":0,"zVsAG968dy8xHxpn":3},"type":"feat","data":{"description":{"value":"<p><em><span id=\"ctl00_MainContent_DataListTypes_ctl159_LabelType\">You have a keen ear for the movements of thieves and monsters.</span></em></p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl159_LabelType\">You can always take 10 on Perception checks, even when stress and distractions would normally prevent you from doing so.</span></p>","chat":"","unidentified":""},"tags":[["Social"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"savannaChildPlains","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Wiscrani Ear"},"flags":{},"img":"systems/pf1/icons/feats/persuasive.jpg","eff