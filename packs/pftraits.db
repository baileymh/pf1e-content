{"_id":"z2MBFjBxp5YsfEpE","name":"Wharf Rat (Ostenso)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>You may never have been on a boat larger than a garbage scow, but you've been helping them dock all your life.</em></p>\n<p>&nbsp;</p>\n<p><strong>Requirements</strong></p>\n<p>Ostenso</p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl437_LabelType\">You gain a +2 trait bonus on skill checks involving climbing and rope use, and a +4 trait bonus on Appraise checks to determine the value of large quantities of bulk goods without opening their containers.</span></p>","chat":"","unidentified":""},"tags":[["Regional"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"wharfRat","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals"