{"_id":"ahM9PghA9456Z1hn","name":"Master of Peaks (the Wall of Heaven)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>You have made it to the summits of a number of the Wall of Heaven's peaks, and your body is used to the effects of both extreme cold and high altitude.\n\t</p></i><b>Benefit</b><p>You gain a +2 trait bonus on Fortitude saving throws made against the effects of cold dangers and altitude sickness.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Regional"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"masterOfPeaksTheWallOfHeaven","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Master of Peaks (the Wall of Heaven)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img