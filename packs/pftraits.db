{"_id":"50oCh8dbHjilgIVw","name":"Speech Of The Wilds (Aasimar; Idyllkin)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>Like your agathion ancestor, you have a knack for communicating with other creatures, and you are adept at breaking down language barriers.</em></p>\n<p>&nbsp;</p>\n<p><strong>Requirements</strong></p>\n<p>Idyllkin Aasimar Heritage</p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p>You can speak one extra language.</p>","chat":"","unidentified":""},"tags":[["Race"],["Aasimar"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"speechOfTheWildsAasimarIdyllkin","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Speech Of The Wilds (Aasimar; Idyllkin)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"systems/pf1/icons/skills/blue_35.