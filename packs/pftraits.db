{"_id":"DBdwI4VZjsH4yOv3","name":"Weathered Emissary","permission":{"default":0,"zVsAG968dy8xHxpn":3},"type":"feat","data":{"description":{"value":"<p><em><span id=\"ctl00_MainContent_DataListTypes_ctl157_LabelType\">Your travels in the wildlands gave you insight into how to survive their dangers and communicate with their inhabitants.</span></em></p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl157_LabelType\">You gain a +1 trait bonus on all Linguistics and Survival checks, and Linguistics is always a class skill for you.</span></p>","chat":"","unidentified":""},"tags":[["Social"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"savannaChildPlains","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Weathered Emissary"