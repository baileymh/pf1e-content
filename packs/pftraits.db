{"_id":"hHqyigaIRpRaULYj","name":"Bureaucrat's Favored (Black Dome)","permission":{"default":0,"zVsAG968dy8xHxpn":3},"type":"feat","data":{"description":{"value":"<p><em><span id=\"ctl00_MainContent_DataListTypes_ctl19_LabelType\">You have gained favor with the government of Sothis.</span></em></p>\n<p>&nbsp;</p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl19_LabelType\"><strong>Requirements</strong></span></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl19_LabelType\">Associated with the court of the Black Dome in Osiria</span></p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p><span id=\"ctl00_MainContent_DataListTypes_ctl19_LabelType\">Once per week, you can attempt a DC 20 Diplomacy or Intimidate check to gain access to an exclusive site or event, such as a government building or an aristocrat&rsquo;s ball.</span></p>","chat":"","unidentified":""},"tags":[["Social"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"savannaChildPlains","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],