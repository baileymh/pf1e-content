{"_id":"d0WH8yHgk76gR0GW","name":"Prideful Temper (Tiefling)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>Memories of the cruel abuse and taunts you suffered as a child cause you to strike back with great fury at anyone who slanders you.</em></p>\n<p>&nbsp;</p>\n<p><strong>Requirements</strong></p>\n<p>Tiefling</p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p>You gain a +1 trait bonus on attack rolls made against creatures that have insulted you until you actually succeed in dealing damage.</p>","chat":"","unidentified":""},"tags":[["Race"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"pridefulTemperTiefling","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Prideful Temper (Tiefling)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"syste