{"_id":"KlwS1tJ7WyRBohli","name":"Berserker of the Society (Barbarian, Pathfinder Society)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>Your time spent as a Pathfinder has taught you new truths about the origins of the your rage ability.</p></em>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p>You may use your rage ability for 3 additional rounds per day.</p>","chat":"","unidentified":""},"tags":[["Combat"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"berserkerOfTheSocietyBarbarianPathfinderSociety","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]}},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"icons/weapons/swords/swords-short.webp","effects":[]}
