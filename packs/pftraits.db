{"_id":"SvjZYOaDp3EMKBST","name":"Bred for War (Shoanti)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<p><em>You tower above most other humans and possess a physique of hard, corded muscle.</p></em>\n<p>&nbsp;</p>\n<p><strong>Requirements</strong></p>\n<p>Shoanti</p>\n<p>&nbsp;</p>\n<p><strong>Benefits</strong></p>\n<p>You gain a +1 trait bonus on Intimidate checks and a +1 trait bonus on your CMB because of your great size. You must be at least 6 feet tall.</p>","chat":"","unidentified":""},"tags":[["Race"],["Human"],["Shoanti"]],"activation":{"cost":0,"type":"","condition":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":"","save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"bredForWarShoanti","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false},"attack":{"parts":[]},"conditionals":[],"identifiedName":"Bred for War (Shoanti)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"systems/pf1/icons/skills/blue_35.jpg","e