{"_id":"E3aJ19DMXh4Jtr5D","name":"Mammoth Master (Realm of the Mammoth Lords)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>You have experience trapping and training large creatures such as mammoths, mastodons, woolly rhinoceroses, and other megafauna.\n\t</p></i><b>Benefit</b><p>You gain a +4 trait bonus on Handle Animal checks related to these creatures.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Regional"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"mammothMasterRealmOfTheMammothLords","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Mammoth Master (Realm of the Mammoth Lords)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"icons/environment/settlement/watchtowe