{"_id":"UmDSrzYHdvwZBwFs","name":"Strong Swimmer (Gozreh)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\"><i><p>Your faith in Gozreh's grace gives you the confidence to swim without fear.\n\t</p></i><b>Benefit</b><p>You gain a +2 trait bonus on all Swim checks, and can hold your breath for an extra 2 rounds when underwater.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Religion"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"strongSwimmerGozreh","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Strong Swimmer (Gozreh)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},"img":"icons/environment/settlement/temple-night.webp","effects":[]}
{"_id":"Uw5ZDWZhGGkKn75T","name":"Repulsive (Tiefling; Foulspawn)","permission":{"default":0},"type":"feat","data":{"description":{"value":"<text type=\"formattedtext\">  <prerequisites type=\"string\">\n\tFoulspawn Tiefling Heritage\n\t  </prerequisites>\n\t<i><p>Your repulsiveness causes people to almost instinctively shy away from making physical contact with you.\n\t</p></i><b>Benefit</b><p>You gain a +1 trait bonus on combat maneuver checks made to overrun and reposition opponents.</p>\n\t  </text>\n\t","chat":"","unidentified":""},"activation":{"type":"","cost":0,"condition":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"ability":"","actionType":"","chatFlavor":"","formula":"","requirements":"","recharge":{"value":null,"charged":false},"featType":"trait","tags":[["Race"]],"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"repulsiveTieflingFoulspawn","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","attack":{"parts":[]},"conditionals":[],"identifiedName":"Repulsive (Tiefling; Foulspawn)"},"folder":"ex0HRF3LEDFUoJVd","flags":{},