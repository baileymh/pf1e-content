{"_id":"AjkiW6RI3joXEGla","name":"Weapon Finesse (EitR)","permission":{"default":0,"T7ruBbHuMiFYUDxv":3},"type":"feat","data":{"description":{"value":"<p><em>You are trained in using your agility in melee combat, as opposed to brute strength.</em> <br /><br /></p>\n<p><strong>Benefits</strong></p>\n<p class=\"stat-block-1\">With a light weapon made for a creature of your size category, you may use your Dexterity modifier instead of your Strength modifier on attack rolls. If you carry a shield, its armor check penalty applies to your attack rolls.</p>\n<p>&nbsp;</p>\n<p><strong>Special</strong></p>\n<p class=\"stat-block-1\">Natural weapons are considered light weapons.</p>","chat":"","unidentified":""},"tags":[["Combat"],["Weapon"],["Elephant in the Room"]],"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"0","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[{"_id":"5p58pans","formula":"@abilities.dex.mod","operator":"add","target":"attack","subTarget":"mattack","modifier":"untyped","priority":0,"value":0},{"_id":"brkq0aea","formula":"-(@abilities.str.mod)","operator":"add","target":"attack","subTarget":"mattack","modifier":"untyped","priority":0,"value":0}],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"weaponFinesseEitr","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false,"_deprecated":true},"time":{"value":"","_deprecated":true},"damageType":{"value":"","_deprecated":true},"attack":{"parts":[]}},"flags":{},"img":"systems/ffd20/icons/feats/weapon-finesse.jpg","effects":[]}
{"_id":"CFkHvO2YUZL6WQqH","name":"Martial Mastery (EitR)","permission":{"default":0,"T7ruBbHuMiFYUDxv":3},"type":"feat","data":{"description":{"value":"<p><em>You further broaden your study of weapons to encompass multiple similar weapons.</em></p>\n<p>&nbsp;</p>\n<p><strong>Prerequisites</strong></p>\n<p>Martial Versatility, fighter level 16th, human.</p>\n<p>&nbsp;</p>\n<p><strong>Benefit</strong></p>\n<p>Each combat feat you have that applies to a specific weapon (e.g., Weapon Focus) can be used with all weapons in the same weapon group.</p>","chat":"","unidentified":""},"tags":[["Weapon"],["Elephant in the Room"],["Combat"]],"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":"","long":null},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"measureTemplate":{"type":"","size":"0","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","changes":[],"changeFlags":{"loseDexToAC":false,"noStr":false,"noDex":false,"oneInt":false,"oneWis":false,"oneCha":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"martialMasteryEitr","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","recharge":{"value":null,"charged":false,"_deprecated":true},"time":{"va