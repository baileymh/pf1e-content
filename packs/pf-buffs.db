{"_id":"Zom7V8sNXZF2ML0M","name":"Protection From Arrows","type":"buff","img":"icons/weapons/ammunition/arrows-fletching.webp","system":{"description":{"value":"<p>You gain damage reduction 10/magic against ranged weapons. This spell doesn&rsquo;t grant you the ability to damage creatures with similar damage reduction. Once the spell has prevented a total of 10 points of damage per caster level (maximum 100 points), it is discharged.</p>","chat":"","unidentified":""},"tags":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[{"text":"<b>Protection From Arrows:</b> DR 10/magic vs ranged weapons.","target":"misc","subTarget":"ac"}],"links":{"children":[]},"tag":"protectionFromArrows","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"buffType":"temp","active":false,"level":"","duration":{"value":null,"units":""},"hideFromToken":false,"uses":{"value":0,"max":0},"identifiedName":"Protection From Arrows","range":{"maxIncrements":1}},"ownership":{"default":0},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1665428266050,"modifiedTime":1677435308082,"lastModifiedBy":"IPeKQhMz2Odi77tR"}}
