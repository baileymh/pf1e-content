ack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"links":{"children":[],"charges":[]},"tag":"hennin","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"masterwork":false,"enh":null,"attackType":"weapon","nonlethal":false,"proficient":true,"primaryAttack":true,"held":"normal","equipmentType":"misc","equipmentSubtype":"clothing","armor":{"value":0,"dex":null,"acp":0,"enh":0},"spellFailure":0,"slot":"head","attack":{"parts":[]}},"sort":2900000,"flags":{},"img":"icons/equipment/head/hat-cocked-feather-green.webp","effects":[],"folder":null,"permission":{"default":0}},{"_id":"GOTvLolwJNM8bCHH","name":"Hollow-heeled boot","type":"equipment","data":{"description":{"value":"<p><strong>Source</strong> <em>Heroes of the Streets pg. 13</em><br/><strong>Price</strong> 10 gp; <strong>Weight</strong> 1 lb.<br/><strong>Category</strong> Clothing</p><h2 class=\"framing\"><img src=\"https://www.aonprd.com/images/PathfinderSocietySymbol.gif\" style=\"margin:3px 3px 0px 3px;\" title=\"PFS Legal\"/>Description</h2>This boot contains a hollow that grants a +4 bonus on Sleight of Hand checks to conceal items within the boot while wearing it. The hollow can hold up to 8 cubic inches. Putting on or removing the boot is a standard action, as is retrieving or storing an item from the hollow when the boot is in hand. Adding an item to or removing one from the boot while it is worn is a full-round action.","chat":"","unidentified":""},"tags":[],"quantity":1,"weight":1,"price":10,"size":"med","equipped":true,"resizing":false,"identified":true,"hp":{"max":10,"value":10},"broken":false,"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"","cl":0,"aura":{"custom":false,"school":""},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":