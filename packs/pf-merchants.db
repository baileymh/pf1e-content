>Construction</h2><strong>Craft (Alchemy)</strong> DC 25","chat":"","unidentified":""},"tags":[],"quantity":1,"weight":1,"price":50,"identified":true,"hp":{"max":10,"value":10},"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"","cl":0,"aura":{"custom":false,"school":""},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":null,"attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","links":{"children":[],"charges":[]},"tag":"pathstones","useCustomTag":false,"flags":{"boolean":[],"dictionary":[["Category","Alchemical Tools"]]},"consumableType":"misc","attack":{"parts":[]}},"sort":20800000,"flags":{},"img":"icons/commodities/gems/gem-fragments-turquoise.webp","effects":[]},{"_id":"dxM1o9Lil0LHZQVA","name":"Perfume/Cologne","type":"consumable","data":{"description":{"value":"<h1 class=\"title\">Perfume/Cologne</h1><strong>Source</strong> <em>Inner Sea World Guide pg. 293</em>, <em>Pathfinder Campaign Setting pg. 213</em>, <em>Curse of the Crimson Throne Player's Guide pg. 12</em><br/><strong>Price</strong> <img src=\"https://www.aonprd.com/images/PathfinderSocietySymbol.gif\" style=\"margin:3px 3px 0px 3px;\" title=\"PFS Legal\"/> 1 gp/dose Perfume (common), <img src=\"https://w