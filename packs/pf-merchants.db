 but you also take a -2 penalty on Acrobatics and Stealth checks.","chat":"","unidentified":""},"tags":[],"quantity":1,"weight":1,"price":8,"identified":true,"hp":{"max":10,"value":10},"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"","cl":0,"aura":{"custom":false,"school":""},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"max":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":null,"attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","links":{"children":[],"charges":[]},"tag":"trackobscuringSoles","useCustomTag":false,"flags":{"boolean":[],"dictionary":[]},"equipped":true,"equipmentType":"misc","equipmentSubtype":"clothing","armor":{"value":0,"dex":null,"acp":0,"enh":0},"spellFailure":0,"slot":"feet","masterwork":false,"size":"med","broken":false,"nonlethal":false,"attack":{"parts":[]}},"sort":9200000,"flags":{},"img":"icons/equipment/feet/shoes-simple-leaf-green.webp","effects":[]},{"_id":"vpuEWrd40B8S8PnD","name":"Veil","type":"equipment","data":{"description":{"value":"<h1 class=\"title\"><img src=\"https://www.aonprd.com/images/PathfinderSocietySymbol.gif\" style=\"margin:3px 3px 0px 3px;