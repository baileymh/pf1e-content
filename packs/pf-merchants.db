 pockets and small hidey-holes. It the wearer a +2 circumstance bonus on Sleight of Hand checks made to conceal a small object on her body.","chat":"","unidentified":""},"tags":[],"quantity":1,"weight":8,"price":20,"size":"med","equipped":true,"resizing":false,"identified":true,"hp":{"max":10,"value":10},"broken":false,"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"","cl":0,"aura":{"custom":false,"school":""},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1","maxFormula":"","max":0},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":null,"attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"links":{"children":[],"charges":[]},"tag":"outfitDilettantes","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"masterwork":false,"enh":null,"attackType":"weapon","nonlethal":false,"proficient":true,"primaryAttack":true,"held":"normal","equipmentType":"misc","equipmentSubtype":"clothing","armor":{"value":0,"dex":null,"acp":0,"enh":0},"spellFailure":0,"slot":"body","attack":{"parts":[]}},"sort":5300000,"flags":{},"img":"