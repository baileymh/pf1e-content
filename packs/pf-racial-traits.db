{"_id":"xjUswZbTpNJTr2gy","name":"Spell-Like Abilities (Duergar)","type":"feat","img":"systems/pf1/icons/races/android.png","flags":{"scene-packer":{"sourceId":"Item.9FaZAsOlt3sj15ww"}},"system":{"description":{"value":"<p><strong>Race:</strong> Duergar&nbsp;</p>\n<hr />\n<p>A duergar can use <em>enlarge person</em> and <em>invisibility</em> each once per day, using her character level as her caster level and affecting itself only.</p>","chat":"","unidentified":""},"tags":[["Duergar"]],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"spelllikeAbilitiesDuergar","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"featType":"racial","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"actionType":null,"ability":{"damageMult":1,"critMult":2},"attack":{},"range":{"maxIncrements":1}},"ownership":{"default":0},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1665428384889,"modifiedTime":1677435557459,"lastModifiedBy":"IPeKQhMz2Odi77tR"}}
