]] Cha"],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"naturalAttack":{"primaryAttack":true,"secondary":{"attackBonus":"-5","damageMult":0.5}},"nonlethal":false,"usesAmmo":false,"spellEffect":"","spellArea":"","conditionals":[],"enh":{"value":null}}],"uses":{"per":"single","value":0,"maxFormula":"","pricePerUse":0,"autoDeductChargesCost":"1","max":0},"attackNotes":[],"effectNotes":[],"links":{"children":[],"charges":[]},"tag":"ungolDust","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"consumableType":"poison","showInQuickbar":false,"activation":{"cost":1,"type":"standard"},"unchainedAction":{"activation":{"cost":2,"type":"action"}},"duration":{"value":null,"units":""},"actionType":"save","ability":{"damage":"","damageMult":1,"critMult":2},"save":{"dc":"15","type":"fort"},"attack":{},"range":{"maxIncrements":1}},"ownership":{"default":0},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1665428366761,"modifiedTime":1677435527001,"lastModifiedBy":"IPeKQhMz2Odi77tR"}}
{"_id":"Ogtkgv2XSuPhvWLb","name":"Starving nettle","type":"consumable","img":"systems/pf1/icons/items/potions/unique-9.jpg","system":{"description":{"value":"<h3>Starving nettle</h3><p><strong>Source</strong> <em>Ultimate Wilderness pg. 145</em></p><p><strong>Price</strong> 300 gp; <strong>Weight</strong> 0</p><p><strong>Type</strong> injury; <strong>Save</strong> Fortitude DC 15</p><p><strong>Frequency</strong> 1/day for 8 days</p><p><strong>Effect</strong> 1d3 Dex and Con damage. In addition, each time the creature fails a save against the poison, for the following 24 hours it is overwhelmed with nausea whenever food is presented to it and is unable to bring itself to eat, no matter how hungry it becomes.</p><p><strong>Cure</strong> 2 consecutive saves</p><h4 class=\"framing\"><strong>Description</strong><h4><p>This peculiar nettle plant bears a singularly effective poison that protects it from being eaten: creatures pricked by the nettle’s thorns and affected by the poison are overcome with nausea at the mere idea of food, protecting the plant from being consumed. Farmers remain vigilant for signs of this fast-growing and hardy plant; it is difficult and dangerous to remove, so culling it is best done early, while the plant is still relatively small.</p>","chat":"","unidentified":""},"tags":[],"quantity":1,"weight":{"value":0},"price":300,"size":"med","equipped":true,"resizing":false,"identified":true,"hp":{"max":10,"value":10},"broken":false,"hardness":0,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"Starving nettle","cl":0,"aura":{"custom":false,"school":""},"actions":[{"_id":"r16mgrr73zenvlt5","name":"Use","img":"systems/pf1/icons/items/potions/unique-9.jpg","description":"","tag":"starvingNettle","activation":{"cost":1,"type":"standard"},"unchainedAction":{"activation":{"cost":2,"type":"action"}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"